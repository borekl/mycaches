<!doctype html>

<html lang="en">

<head>
	<meta charset='utf-8'>
	<meta name='viewport' content='width=device-width,initial-scale=1'>

	<title>MyCaches::Hide</title>

	<link rel='stylesheet' href="<%= url_for '/app/global.css' %>">
	<link rel='stylesheet' href="<%= url_for '/app/build/bundle.css' %>">

	<script defer src="<%= url_for '/app/build/bundle.js' %>"></script>
</head>

<body
  id="app"
  data-id="<%= $hide->{hides_i} // 'new' %>"
  data-prefix="<%= url_for '/' %>"
  data-backref="<%= url_for '/hides' %>"
  data-type="hide"
></body>

</html>
